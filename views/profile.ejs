<%- include('header.ejs') %>

<h1><%= profile.first_name %> <%= profile.last_name %></h1>

<img
  src="/uploads/<%= profile.profile_picture %>"
  alt="<%= profile.first_name %>'s profile picture"
>

<dl>
  <dt>Gender</dt>
  <dd><%= profile.gender %></dd>
  <dt>Age</dt>
  <dd><%= profile.birthday %></dd>
</dl>

<p><%= profile.description %></p>

<%- include('footer.ejs') %>
